<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import java.net.URL?>

<?import javafx.scene.control.Label?>
<Pane xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.lionfish.GUI.views.PromotionView"
      prefHeight="104.0" prefWidth="356.0"
      fx:id="pane">
    <stylesheets>
        <URL value="@../style.css"/>
    </stylesheets>
    <VBox alignment="CENTER" prefWidth="356.0">
        <Label>Choose a piece</Label>
        <HBox alignment="CENTER" prefHeight="104.0">
            <GridPane fx:id="boardGrid"/>
        </HBox>
    </VBox>
</Pane>
